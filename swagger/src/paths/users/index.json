{
    "tags": [
        "User"
    ],
    "description": "ユーザーを取得する。",
    "parameters": [
        {
            "description": "ページの指定",
            "in": "query",
            "name": "page",
            "required": false,
            "schema": {
                "type": "integer",
                "example": 1
            }
        },
        {
            "description": "1ページに表示する件数",
            "in": "query",
            "name": "per_page",
            "required": false,
            "schema": {
                "type": "integer",
                "example": 5
            }
        },
        {
            "description": "検索キーワード",
            "in": "query",
            "name": "keyword",
            "required": false,
            "schema": {
                "type": "string",
                "example": "郎"
            }
        }
    ],
    "responses": {
        "200": {
            "description": "成功",
            "content": {
                "application/json": {
                    "examples": {
                        "general": {
                            "summary": "一般ユーザー実行時のレスポンス",
                            "value": {
                                "$ref": "../../resources/users/multiple/general.json"
                            }
                        },
                        "admin": {
                            "summary": "管理者ユーザー実行時のレスポンス",
                            "value": {
                                "$ref": "../../resources/users/multiple/admin.json"
                            }
                        }
                    }
                }
            }
        }
    }
}
