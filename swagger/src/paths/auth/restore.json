{
    "tags": [
        "Auth"
    ],
    "description": "メールアドレスとパスワードをもとに、指定したユーザーを復活させる。",
    "requestBody": {
        "content": {
            "application/json:": {
                "schema": {
                    "type": "object",
                    "properties": {
                        "email": {
                            "type": "string",
                            "example": "test@example.com"
                        },
                        "password": {
                            "type": "string",
                            "example": "password"
                        }
                    },
                    "required": [
                        "email",
                        "password"
                    ]
                }
            }
        }
    },
    "responses": {
        "204": {
            "description": "成功"
        },
        "419": {
            "description": "CORS not allowed"
        }
    }
}
