{
    "tags": [
        "Auth"
    ],
    "description": "トークンをもとにユーザーを検索し、対象ユーザーを復活させる。",
    "requestBody": {
        "content": {
            "application/json:": {
                "schema": {
                    "type": "object",
                    "properties": {
                        "restore_token": {
                            "type": "string",
                            "example": "2UWS1KkuBUAe0oot9hOYtJrqZUUnUykRR6EruXJU9vL4TnMm2CXLwpNFoIAj"
                        }
                    },
                    "required": [
                        "restore_token"
                    ]
                }
            }
        }
    },
    "responses": {
        "204": {
            "description": "成功"
        },
        "419": {
            "description": "CORS not allowed"
        },
        "422": {
            "description": "validation error"
        }
    }
}
