{
    "tags": [
        "Auth"
    ],
    "description": "トークンをもとにユーザーを検索し、対象ユーザーを復活させる。",
    "parameters": [
        {
            "description": "ユーザー復活用トークン",
            "in": "path",
            "name": "token",
            "required": true,
            "schema": {
                "type": "string",
                "example": "2UWS1KkuBUAe0oot9hOYtJrqZUUnUykRR6EruXJU9vL4TnMm2CXLwpNFoIAj"
            }
        }
    ],
    "responses": {
        "204": {
            "description": "成功"
        },
        "419": {
            "description": "CORS not allowed"
        },
        "422": {
            "description": "validation error"
        }
    }
}
