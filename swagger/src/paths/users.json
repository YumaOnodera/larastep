{
    "get": {
        "title": "ユーザー一覧",
        "description": "全ユーザーの取得",
        "responses": {
            "200": {
                "description": "成功",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "users": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "email": {
                                                "type": "string",
                                                "example": "test@example.co.jp"
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "山田太郎"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "example": "2022-01-01 00:00:00"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "example": "2022-01-01 00:00:00"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
